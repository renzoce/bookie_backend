module.exports = function(sequelize, DataTypes) {

  var Owner = sequelize.define('Owner', {
    rut: {
      type: DataTypes.STRING
    },
    documentCI: {
      type: DataTypes.INTEGER
    }
  }, {
    classMethods: {
      associate: function(models) {
        Owner.belongsTo(models.User, {
          //foreignKey: 'userId'
        })
      }
    }
  });
  return Owner;
};
